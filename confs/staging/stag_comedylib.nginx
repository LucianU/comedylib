server {
    listen 80;
    server_name test.comedylib.com;

    client_max_body_size 20M;

    location / {
        uwsgi_pass unix:///home/comedylib/stag_comedylib/var/run/comedylib.sock;
        include uwsgi_params;
    }

    location /static {
        alias /home/comedylib/stag_comedylib/comedylib/static/;
    }
}
